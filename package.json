  "keywords": [
    "darcula",
    "dark-theme",
    "dracula",
    "theme",
    "zed"
  ],
  "license": "MIT",
  "author": "Himenon <6715229+Himenon@users.noreply.github.com>",
  "contributors": [
    {
      "name": "kevinvn1709",
      "url": "https://github.com/kevinvn1709"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Himenon/zed-theme-darcula-darker"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "postinstall": "lefthook install",
    "format": "oxfmt && prettier --write **/*.md"
